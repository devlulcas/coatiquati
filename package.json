{
	"name": "coati",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:mg-gen": "drizzle-kit generate:pg --config=drizzle.config.ts",
		"db:mg-drop": "drizzle-kit drop --config=drizzle.config.ts",
		"db:metadata-update": "drizzle-kit up:pg --config=drizzle.config.ts",
		"db:introspect": "drizzle-kit introspect:pg --config=drizzle.config.ts",
		"db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
		"db:mg-run": "tsx src/lib/server/db/migration.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.36.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.3",
		"@types/nodemailer": "^6.4.8",
		"@types/nprogress": "^0.2.0",
		"@types/pg": "^8.10.2",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.14",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.18.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pino-pretty": "^10.0.1",
		"postcss": "^8.4.26",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.6",
		"sveltekit-superforms": "1.0.0-rc.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.0",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vite": "^4.4.4",
		"vitest": "^0.29.8"
	},
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "^1.0.1",
		"@lucia-auth/oauth": "^1.2.1",
		"@lucia-auth/sveltekit": "^0.6.11",
		"class-variance-authority": "^0.6.1",
		"cloudinary": "^1.37.3",
		"clsx": "^1.2.1",
		"drizzle-orm": "^0.26.5",
		"form-data": "^4.0.0",
		"lucia-auth": "^1.8.0",
		"lucide-svelte": "^0.219.0",
		"mailgun.js": "^8.2.2",
		"nodemailer": "^6.9.3",
		"nprogress": "^0.2.0",
		"pg": "^8.11.1",
		"pino": "^8.14.1",
		"tailwind-merge": "^1.14.0",
		"uuid": "^9.0.0",
		"zod": "^3.21.4"
	}
}
