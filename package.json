{
	"name": "coati",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:mg-gen": "drizzle-kit generate:pg --config=drizzle.config.ts",
		"db:mg-drop": "drizzle-kit drop --config=drizzle.config.ts",
		"db:metadata-update": "drizzle-kit up:pg --config=drizzle.config.ts",
		"db:introspect": "drizzle-kit introspect:pg --config=drizzle.config.ts",
		"db:push": "drizzle-kit push:mysql --config=drizzle.config.ts",
		"db:mg-run": "tsx src/lib/server/db/migration.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.0",
		"@types/nodemailer": "^6.4.8",
		"@types/pg": "^8.10.2",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"autoprefixer": "^10.4.14",
		"dotenv": "^16.1.4",
		"drizzle-kit": "^0.18.1",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pino-pretty": "^10.0.0",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "1.0.0-rc.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"tsx": "^3.12.7",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.29.8"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "^1.0.1",
		"@lucia-auth/oauth": "^1.1.1",
		"@lucia-auth/sveltekit": "^0.6.11",
		"class-variance-authority": "^0.6.0",
		"clsx": "^1.2.1",
		"drizzle-orm": "^0.26.5",
		"embla-carousel-svelte": "8.0.0-rc03",
		"form-data": "^4.0.0",
		"lucia-auth": "^1.7.1",
		"lucide-svelte": "^0.219.0",
		"mailgun.js": "^8.2.2",
		"micromark": "^3.1.0",
		"micromark-extension-gfm": "^2.0.3",
		"nanoid": "^4.0.2",
		"nodemailer": "^6.9.3",
		"pg": "^8.11.0",
		"pino": "^8.14.1",
		"svelte-droplet": "^0.3.0",
		"tailwind-merge": "^1.12.0",
		"zod": "^3.21.4"
	}
}
