{
	"cSpell.words": ["cloudinary", "dtos", "embla", "ilike", "lucia", "micromark", "postgres", "sqlite"],
	"cSpell.language": "en,pt-BR",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": false
	},
	"editor.formatOnSave": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"sqltools.connections": [
		{
			"previewLimit": 50,
			"server": "localhost",
			"driver": "PostgreSQL",
			"name": "coati",
			"group": "tcc",
			"connectString": "postgresql://postgres:postgres@localhost:5432/db-coati"
		}
	]
}
